def greedy():
    k = 10
    T = 1000
    Qt=np.zeros(k)
    N=np.zeros(k)
    Reward=np.zeros(k)
    for i in range(T):
        Action=np.argmax(Qt)
        mu=np.random.normal(0,1)
        Reward[Action]=np.random.normal(mu,1)
        N[Action]=N[Action]+1
        Qt[Action]=Qt[Action]+(Reward[Action]-Qt[Action])

    return Qt,Reward

def main():
    Action_value,Reward=greedy()
    plt.figure("Greedy")
    #plt.suptitle("Time plots last 50 data packets")
